      "id": "may-26-1981-am-canvas-1-reporter-annotation-19.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Your wife died. Who else?",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "El_Junquillo",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-english/may-26-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "1884.0,1886.0"
        }
      }
    },
    {
