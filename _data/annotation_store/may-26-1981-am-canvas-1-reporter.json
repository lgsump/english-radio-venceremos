    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "may-26-1981-am-canvas-1-reporter-annotation-3.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Do you know how many children they killed here in this zone?",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "El_Junquillo",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-english/may-26-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
