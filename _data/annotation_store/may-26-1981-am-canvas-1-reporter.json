          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Morazán",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-english/may-26-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "1818.0,1836.0"
        }
