        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "may-26-1981-am-canvas-1-survivor-annotation-12.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Spies from Cacaopera.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Morazán",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
