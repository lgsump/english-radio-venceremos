    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "1981-05-27-annotations-part-1-canvas-1-music-annotation-11.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "<i>We will give our lives for the fatherland because we will endure liberty. The fatherland in this land we will overcome. Working, day by day, we will live. No more tyrants and assassins.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://radiovenceremos.github.io/radio-venceremos-english-1/1981-05-27-annotations-part-1/canvas-1/canvas",
        "selector": {
