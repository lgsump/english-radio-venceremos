    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "may-26-1981-am-canvas-1-reporter-annotation-18.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "In this opportunity we’ve found a peasant from this area, and his name is Filadelfo. In these moments we’re arriving at a house were his wife and children lived. It’s a return to a painful memory that Filadelfo has wanted to make so he could show the world all the horror and the entire genocidal nature of what happened in this area of El Junquillo. Comrade, tell us how many people lived in this house.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "El_Junquillo",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-english/may-26-1981-am/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "1837.0,1876.0"
        }
      }
    },
    {
