      "id": "may-26-1981-am-canvas-1-reporter-annotation-15.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "¿From Cacaopera? ¿Along with Medina Garay?",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://lgsump.github.io/radio-venceremos-english/may-26-1981-am/canvas-1/canvas",
