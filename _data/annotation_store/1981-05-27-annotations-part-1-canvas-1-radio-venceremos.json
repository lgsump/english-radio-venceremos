    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "1981-05-27-annotations-part-1-canvas-1-radio-venceremos-annotation-7.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "For our part, we retreated after we completed this operation without suffering any casualties. On the 17th of May at nine at night, the patriotic forces of the FMLN carried out an attack on the Death Squadron in San Martin Street in Suchitoto.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://radiovenceremos.github.io/radio-venceremos-english/1981-05-27-annotations-part-1/canvas-1/canvas",
