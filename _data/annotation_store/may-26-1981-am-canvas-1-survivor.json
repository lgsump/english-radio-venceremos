      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "Yes.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": "https://radiovenceremos.github.io/radio-venceremos-english/may-26-1981-am/canvas-1/canvas",
        "selector": {
          "type": "PointSelector",
